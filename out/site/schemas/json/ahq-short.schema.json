        "governmental:city",
        "governmental:township",
        "governmental:hospital_district",
        "governmental:other",
        "nonprofit:church_related",
        "nonprofit:corporation_not_church",
        "nonprofit:other",
        "for_profit:corporation",
        "for_profit:limited_partnership",
        "for_profit:llp",
        "for_profit:llc",
        "for_profit:other"
