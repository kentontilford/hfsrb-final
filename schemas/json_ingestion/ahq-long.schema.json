  "properties": {
    "license_idph": {
      "type": "string",
      "description": "Facility IDPH License Number (Section/Page: p.1)"
    },
    "facility_name": {
      "type": "string",
      "description": "Facility Name (Section/Page: p.1)"
    },
    "address_line1": {
      "type": "string",
      "description": "Facility Address (Section/Page: p.1)"
    },
    "address_city": {
      "type": "string",
      "description": "Facility City (Section/Page: p.1)"
    },
    "address_zip": {
      "type": "string",
      "pattern": "^\\d{5}(-\\d{4})?$",
      "description": "Facility Zip Code (Section/Page: p.1)"
    },
    "fein": {
      "type": "string",
      "pattern": "^\\d{2}-\\d{7}$",
      "description": "Facility FEIN Number (Section/Page: p.1)"
    },
    "operator_entity": {
      "type": "string",
      "description": "Legal entity operating the facility (Section/Page: p.1)"
    },
    "plant_owner": {
      "type": "string",
      "description": "Legal entity owning physical plant (Section/Page: p.1)"
    },
    "hospital_characterization": {
      "type": "array",
      "description": "Entire hospital characterized as (Section/Page: p.2) Multi-select"
    },
    "cms_certification": {
      "type": "string",
      "description": "CMS certification (entire facility) (Section/Page: p.2) If applicable"
    },
    "ownership_type": {
      "type": "string",
      "description": "Managing organization type (Section/Page: p.2) Select one"
    },
    "ownership_other": {
      "type": "string",
      "description": "Other Ownership Details (Section/Page: p.2) Required if “Other …” selected"
    },
    "chna_url": {
      "type": "string",
