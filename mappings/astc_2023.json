  "arrays": [
    {
      "dest": "owners",
      "count": 24,
      "require": ["name"],
      "item": {
        "name": "astcowner{n}",
        "address": "astcowneraddress{n}",
        "city_state_zip": "astcownercitystzip{n}",
        "phone": "astcownerphone{n}"
