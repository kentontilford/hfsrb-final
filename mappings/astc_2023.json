      "astcmale0_14", "astcmale15_44", "astcmale45_64", "astcmale65_74", "astcmale75plus",
      "astcfemale0_14", "astcfemale15_44", "astcfemale45_64", "astcfemale65_74", "astcfemale75plus"
    ],
    "patients_payment_total": [
      "astcmalemedicaid", "astcmalemedicare", "astcmaleothpub", "astcmaleprvins", "astcmaleprivatepay", "astcmalecharcare",
      "astcfemalemedicaid", "astcfemalemedicare", "astcfemaleothpub", "astcfemaleprvins", "astcfemaleprivatepay", "astcfemalecharcare"
    ]
